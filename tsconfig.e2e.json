{
  "extends": "@tsconfig/node22/tsconfig.json",
  "compilerOptions": {
    "composite": true,
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "noEmit": true,
    "skipLibCheck": true,
    "tsBuildInfoFile": "./node_modules/.tmp/tsconfig.e2e.tsbuildinfo",
    "types": ["node"]
  },
  "include": ["e2e/**/*.ts", "e2e/**/*.tsx", "e2e/**/*.mts", "e2e/**/*.cts"]
}
